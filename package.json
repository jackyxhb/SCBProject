{
  "name": "scbproject",
  "private": true,
  "workspaces": ["frontend", "backend"],
  "scripts": {
    "dev": "concurrently -n ui,api -c blue,green \"npm:dev -w frontend\" \"npm:dev -w backend\"",
    "dev:ui": "npm run dev -w frontend",
    "dev:api": "npm run dev -w backend",
  "build": "npm run build -w frontend && npm run build -w backend",
  "vercel-build": "npm run build",
    "test": "npm -w backend test"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
